<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.ruoyi_yiliao.mapper.TMedicalCostPaymentMapper">
    
    <resultMap type="TMedicalCostPayment" id="TMedicalCostPaymentResult">
        <result property="id"    column="id"    />
        <result property="partName"    column="partName"    />
        <result property="partNumber"    column="partNumber"    />
        <result property="outpatientId"    column="outpatientId"    />
        <result property="money"    column="money"    />
        <result property="status"    column="status"    />
        <result property="doctorid"    column="doctorid"    />
        <result property="createTime"    column="createTime"    />
    </resultMap>

    <sql id="selectTMedicalCostPaymentVo">
        select id, partName, partNumber, outpatientId, money, status, doctorid, createTime from t_medical_cost_payment
    </sql>

    <select id="selectTMedicalCostPaymentList" parameterType="TMedicalCostPayment" resultMap="TMedicalCostPaymentResult">
        <include refid="selectTMedicalCostPaymentVo"/>
        <where>  
            <if test="partName != null  and partName != ''"> and partName like concat('%', #{partName}, '%')</if>
            <if test="partNumber != null "> and partNumber = #{partNumber}</if>
            <if test="outpatientId != null  and outpatientId != ''"> and outpatientId = #{outpatientId}</if>
            <if test="money != null "> and money = #{money}</if>
            <if test="status != null "> and status = #{status}</if>
            <if test="doctorid != null  and doctorid != ''"> and doctorid = #{doctorid}</if>
            <if test="createTime != null "> and createTime = #{createTime}</if>
        </where>
    </select>
    
    <select id="selectTMedicalCostPaymentById" parameterType="String" resultMap="TMedicalCostPaymentResult">
        <include refid="selectTMedicalCostPaymentVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertTMedicalCostPayment" parameterType="TMedicalCostPayment">
        insert into t_medical_cost_payment
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="partName != null">partName,</if>
            <if test="partNumber != null">partNumber,</if>
            <if test="outpatientId != null">outpatientId,</if>
            <if test="money != null">money,</if>
            <if test="status != null">status,</if>
            <if test="doctorid != null">doctorid,</if>
            <if test="createTime != null">createTime,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="partName != null">#{partName},</if>
            <if test="partNumber != null">#{partNumber},</if>
            <if test="outpatientId != null">#{outpatientId},</if>
            <if test="money != null">#{money},</if>
            <if test="status != null">#{status},</if>
            <if test="doctorid != null">#{doctorid},</if>
            <if test="createTime != null">#{createTime},</if>
         </trim>
    </insert>

    <update id="updateTMedicalCostPayment" parameterType="TMedicalCostPayment">
        update t_medical_cost_payment
        <trim prefix="SET" suffixOverrides=",">
            <if test="partName != null">partName = #{partName},</if>
            <if test="partNumber != null">partNumber = #{partNumber},</if>
            <if test="outpatientId != null">outpatientId = #{outpatientId},</if>
            <if test="money != null">money = #{money},</if>
            <if test="status != null">status = #{status},</if>
            <if test="doctorid != null">doctorid = #{doctorid},</if>
            <if test="createTime != null">createTime = #{createTime},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTMedicalCostPaymentById" parameterType="String">
        delete from t_medical_cost_payment where id = #{id}
    </delete>

    <delete id="deleteTMedicalCostPaymentByIds" parameterType="String">
        delete from t_medical_cost_payment where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>