<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ruoyi.ruoyi_yiliao.mapper.TMedicalOutpatientdoctorActionMapper">
    
    <resultMap type="TMedicalOutpatientdoctorAction" id="TMedicalOutpatientdoctorActionResult">
        <result property="id"    column="id"    />
        <result property="partName"    column="partName"    />
        <result property="partNumber"    column="partNumber"    />
        <result property="personTyoe"    column="personTyoe"    />
        <result property="partMoney"    column="partMoney"    />
        <result property="type"    column="type"    />
        <result property="opDoctorReceiveRecordId"    column="opDoctorReceiveRecordId"    />
    </resultMap>

    <sql id="selectTMedicalOutpatientdoctorActionVo">
        select id, partName, partNumber, personTyoe, partMoney, type, opDoctorReceiveRecordId from t_medical_outpatientdoctor_action
    </sql>

    <select id="selectTMedicalOutpatientdoctorActionList" parameterType="TMedicalOutpatientdoctorAction" resultMap="TMedicalOutpatientdoctorActionResult">
        <include refid="selectTMedicalOutpatientdoctorActionVo"/>
        <where>  
            <if test="partName != null  and partName != ''"> and partName like concat('%', #{partName}, '%')</if>
            <if test="partNumber != null  and partNumber != ''"> and partNumber = #{partNumber}</if>
            <if test="personTyoe != null "> and personTyoe = #{personTyoe}</if>
            <if test="partMoney != null "> and partMoney = #{partMoney}</if>
            <if test="type != null "> and type = #{type}</if>
            <if test="opDoctorReceiveRecordId != null  and opDoctorReceiveRecordId != ''"> and opDoctorReceiveRecordId = #{opDoctorReceiveRecordId}</if>
        </where>
    </select>
    
    <select id="selectTMedicalOutpatientdoctorActionById" parameterType="String" resultMap="TMedicalOutpatientdoctorActionResult">
        <include refid="selectTMedicalOutpatientdoctorActionVo"/>
        where id = #{id}
    </select>
        
    <insert id="insertTMedicalOutpatientdoctorAction" parameterType="TMedicalOutpatientdoctorAction">
        insert into t_medical_outpatientdoctor_action
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id != null">id,</if>
            <if test="partName != null">partName,</if>
            <if test="partNumber != null">partNumber,</if>
            <if test="personTyoe != null">personTyoe,</if>
            <if test="partMoney != null">partMoney,</if>
            <if test="type != null">type,</if>
            <if test="opDoctorReceiveRecordId != null">opDoctorReceiveRecordId,</if>
         </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="partName != null">#{partName},</if>
            <if test="partNumber != null">#{partNumber},</if>
            <if test="personTyoe != null">#{personTyoe},</if>
            <if test="partMoney != null">#{partMoney},</if>
            <if test="type != null">#{type},</if>
            <if test="opDoctorReceiveRecordId != null">#{opDoctorReceiveRecordId},</if>
         </trim>
    </insert>

    <update id="updateTMedicalOutpatientdoctorAction" parameterType="TMedicalOutpatientdoctorAction">
        update t_medical_outpatientdoctor_action
        <trim prefix="SET" suffixOverrides=",">
            <if test="partName != null">partName = #{partName},</if>
            <if test="partNumber != null">partNumber = #{partNumber},</if>
            <if test="personTyoe != null">personTyoe = #{personTyoe},</if>
            <if test="partMoney != null">partMoney = #{partMoney},</if>
            <if test="type != null">type = #{type},</if>
            <if test="opDoctorReceiveRecordId != null">opDoctorReceiveRecordId = #{opDoctorReceiveRecordId},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteTMedicalOutpatientdoctorActionById" parameterType="String">
        delete from t_medical_outpatientdoctor_action where id = #{id}
    </delete>

    <delete id="deleteTMedicalOutpatientdoctorActionByIds" parameterType="String">
        delete from t_medical_outpatientdoctor_action where id in 
        <foreach item="id" collection="array" open="(" separator="," close=")">
            #{id}
        </foreach>
    </delete>
</mapper>